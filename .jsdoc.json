{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "lib/",
    "exclude": ["lib/amountItems/index.js", "lib/address/index.js", "lib/indicativeArea/index.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
